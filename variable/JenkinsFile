pipeline{
    agent any
    environment{
        def name = "Shameeda"
        def age = 10
        def minor = true
    }
    stages{
        stage("Demo"){
            steps{
                echo " name: ${env.name}\n age: ${env.age}\n minor: ${env.minor} "
                echo " build number: ${env.BUILD_NUMBER}" 
            }
        }
    }
}
